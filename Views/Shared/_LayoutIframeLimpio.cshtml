<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - SIPYME</title>
    
    @Styles.Render("~/Content/Admin")

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <img src="~/Imagenes/logoMuniBlanco.jpg" alt="Sistema de Patentes San Pablo" style="width: 300px; height: auto;"

            </div>


        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr class="d-none d-xxl-block">
        <footer class=" d-none d-xxl-block">
            <p>&copy; @DateTime.Now.Year - Municipalidad de San Pablo de Heredia</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/AdminScript")
    @RenderSection("scripts", required: false)
</body>
</html>
